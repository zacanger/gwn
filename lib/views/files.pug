doctype html
html
  include head.pug
  body
    h1
      a(href='/') /
    h2
      a(href=`/${repo}`)= repo
    a(href=`/${repo}/${files}`)
    ul
      each f in files
        li
          span
            a(href=`/${repo}/file/${encodeURIComponent(f.path)}`)= f.path
            span  &mdash;
            code= f.type
