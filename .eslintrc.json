{
  "root": true,
  "plugins": [
    "zacanger"
  ],
  "extends": [
    "plugin:zacanger/recommended",
    "plugin:zacanger/node",
    "plugin:zacanger/prettier"
  ],
  "rules": {
    "fp/no-let": 0,
    "fp/no-loops": 0,
    "fp/no-mutating-methods": 0,
    "fp/no-mutation": 0,
    "fp/no-nil": 0,
    "fp/no-this": 0,
    "fp/no-unused-expression": 0,
    "func-style": 0,
    "max-params": 0,
    "no-console": 0
  }
}
