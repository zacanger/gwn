    "fetchyeah": "3.0.11",
    "helmet": "3.22.0",
    "is-docker": "2.0.0",
    "is-program-installed": "2.0.6",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.15",
    "minimist": "1.2.6",
    "nodemon": "2.0.2",
    "pug": "2.0.4",
    "url-parse": "1.4.7",
